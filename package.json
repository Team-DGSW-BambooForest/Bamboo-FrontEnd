{
  "name": "bamboo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.26",
    "react-icons": "^4.7.1",
    "recoil": "^0.7.6",
    "styled-components": "^5.3.6"
  },
  "packageManager": "yarn@1.22.19"
}
